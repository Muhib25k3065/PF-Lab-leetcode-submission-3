#include <stdio.h>
#include <string.h>

int main()
{
	char word[50];
	char vowels[50];
	
	printf("Enter a word: ");
	scanf("%s", word);
	
	printf("Input: %s", word);
	
	int i, j = 0;
	int length = strlen(word);
	
	for(i = 0; i < length; i++)
	{
		if(word[i] == 'a' || word[i] == 'A' || word[i] == 'e' || word[i] == 'E' || word[i] == 'i' || word[i] == 'I' || word[i] == 'o' || word[i] == 'O' || word[i] == 'u' || word[i] == 'U')
		{
			vowels[j] = word[i];
			j += 1;
		}
	}
	
	int length2 = strlen(vowels);
	char temp;
	int n = length2;
	int l = 1;
	
	for(i = 0; i < j/2; i++)
	{
		temp = vowels[i];
		vowels[i] = vowels[n-l];
		vowels[n-l] = temp;
		l += 1;
	}
	
	int k = 0;
	
	for(i = 0; i < length; i++)
	{
		if(word[i] == 'a' || word[i] == 'A' || word[i] == 'e' || word[i] == 'E' || word[i] == 'i' || word[i] == 'I' || word[i] == 'o' || word[i] == 'O' || word[i] == 'u' || word[i] == 'U')
		{
			word[i] = vowels[k];
			k += 1;
		}
	}
	
	printf("\nOutput: %s", word);
	
	return 0;
}
